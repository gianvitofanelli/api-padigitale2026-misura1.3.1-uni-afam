{
    "@context": {
       "LEA": "https://w3id.org/italia/onto/Learning/",
       "CPV": "https://w3id.org/italia/onto/CPV/",
       "owl": "http://www.w3.org/2002/07/owl#"
    },
    "@graph" : [
    	{{#each learnings}}
{    
    "@type": "CPV:Person",
    "@id": "https://w3id.org/italia/data/CPV/Person/personId/{{../personal_data.person_id}}",
    "owl:sameAs": "https://w3id.org/italia/data/CPV/Person/taxCode/{{../personal_data.tax_code}}",
    "CPV:taxCode": "{{../personal_data.tax_code}}",
    "CPV:persondId": "{{../personal_data.person_id}}",
    "LEA:hasEnrolment":{
        "@type": "LEA:Enrolment",
        "@id": "https://w3id.org/italia/data/Learning/Enrolment/{{../personal_data.tax_code}}-???",
        "LEA:atInstitute": {
            "@type": "LEA:Institute",
            "@id": "https://w3id.org/italia/data/Learning/Institute/instituteCode/{{institute_code}}",
            "LEA:instituteCode": "{{institute_code}}",
            "LEA:instituteName": "{{institute_name}}"
        },
        "LEA:hasDegreeCourse":{
            "@type": "LEA:DegreeCourse",
            "@id": "https://w3id.org/italia/data/Learning/DegreeCourse/{{institute_code}}-{{degree_course_code}}",
            "LEA:DegreeCourse" : "{{degree_course_code}}",
            "degree_course_name": "???",
            "LEA:hasDegreeClass":{
                "@type": "LEA:DegreeClass",
                "@id": "https://w3id.org/italia/data/Learning/DegreeClass/{{degree_class_code}}",
                "LEA:DegreeClass" : "{{degree_class_code}}",
                "degree_class_name": "???",
                "LEA:hasProgrammeType": "https://w3id.org/italia/data/Learning/ProgrammeType/{{programme_type_code}}",
                "programme_type_name": "???"
            },
        "LEA:enablesLearning": {
            "LEA:includesExamination":{
                "@type": "LEA:Examination",
                "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-1",
                "Examination": "{{attended_learning_name}}",
                "LEA:hasGrade":{
                    "@type": "LEA:Grade",
                    "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-1/{{grade_value}}",
                    "LEA:gradeValue": "28",
                    "LEA:hasGradingScale":{
                        "@type": "LEA:GradingScale",
                        "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-1/GradeScale",
                        "hasMaximumGrade": {
                            "@type": "LEA:Grade",
                            "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-1/GradeScale/GradeMax/{{grading_scale_max}}",
                            "LEA:gradeValue": "30"
                        },
                        "hasMinimumGrade": {
                            "@type": "LEA:Grade",
                            "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-1/GradeScale/GradeMin/{{grading_scale_min}}",
                            "LEA:gradeValue": "18"
                        }
                    }
                },
                "start_date": "???",
                "end_date": "???",
                "ects_credits_received": "???"
            }
        }
        }
    }
},
{    
    "@type": "CPV:Person",
    "@id": "https://w3id.org/italia/data/CPV/Person/personId/{{../personal_data.person_id}}",
    "owl:sameAs": "https://w3id.org/italia/data/CPV/Person/taxCode/{{../personal_data.tax_code}}",
    "CPV:taxCode": "{{../personal_data.tax_code}}",
    "CPV:persondId": "{{../personal_data.person_id}}",
    "LEA:hasEnrolment":{
        "@type": "LEA:Enrolment",
        "@id": "https://w3id.org/italia/data/Learning/Enrolment/{{../personal_data.tax_code}}-???",
        "LEA:atInstitute": {
            "@type": "LEA:Institute",
            "@id": "https://w3id.org/italia/data/Learning/Institute/instituteCode/{{institute_code}}",
            "LEA:instituteCode": "{{institute_code}}",
            "LEA:instituteName": "{{institute_name}}"
        },
        "LEA:hasDegreeCourse":{
            "@type": "LEA:DegreeCourse",
            "@id": "https://w3id.org/italia/data/Learning/DegreeCourse/{{institute_code}}-{{degree_course_code}}",
            "LEA:DegreeCourse" : "{{degree_course_code}}",
            "degree_course_name": "???",
            "LEA:hasDegreeClass":{
                "@type": "LEA:DegreeClass",
                "@id": "https://w3id.org/italia/data/Learning/DegreeClass/{{degree_class_code}}",
                "LEA:DegreeClass" : "{{degree_class_code}}",
                "degree_class_name": "???",
                "LEA:hasProgrammeType": "https://w3id.org/italia/data/Learning/ProgrammeType/{{programme_type_code}}",
                "programme_type_name": "???"
            },
        "LEA:enablesLearning": {
            "LEA:includesExamination":{
                "@type": "LEA:Examination",
                "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-2",
                "Examination": "{{attended_learning_name}}",
                "LEA:hasGrade":{
                    "@type": "LEA:Grade",
                    "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-2/{{grade_value}}",
                    "LEA:gradeValue": "29",
                    "LEA:hasGradingScale":{
                        "@type": "LEA:GradingScale",
                        "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-2/GradeScale",
                        "hasMaximumGrade": {
                            "@type": "LEA:Grade",
                            "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-2/GradeScale/GradeMax/{{grading_scale_max}}",
                            "LEA:gradeValue": "30"
                        },
                        "hasMinimumGrade": {
                            "@type": "LEA:Grade",
                            "@id": "https://w3id.org/italia/data/Learning/Enrolment/Examination/Esame-2/GradeScale/GradeMin/{{grading_scale_min}}",
                            "LEA:gradeValue": "18"
                        }
                    }
                },
                "start_date": "???",
                "end_date": "???",
                "ects_credits_received": "???"
            }
        }
        }
    }
}
{{#unless @last}},{{/unless}}
{{/each}}
]
}
